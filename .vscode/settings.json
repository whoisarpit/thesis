{
  "explorer.excludeGitIgnore": true,
  "latex-workshop.latex.tools": [
    {
      "args": [
        "--shell-escape",
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-pdf",
        "-outdir=%OUTDIR%",
        "%DOC%"
      ],
      "command": "latexmk",
      "env": {},
      "name": "latexmk"
    },
    {
      "args": [
        "--shell-escape",
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"
      ],
      "command": "pdflatex",
      "env": {},
      "name": "pdflatex"
    },
    {
      "args": [
        "--shell-escape",
        "%DOCFILE%"
      ],
      "command": "bibtex",
      "env": {},
      "name": "bibtex"
    }
  ],
  "latex-workshop.view.pdf.color.dark.pageBorderColor": "#6f6f6f1f",
  "latex-workshop.view.pdf.color.dark.backgroundColor": "#161616",
  "latex-workshop.view.pdf.color.dark.pageColorsBackground": "#242424",
  "latex-workshop.view.pdf.color.dark.pageColorsForeground": "#ccc",
  "window.title": "Thesis"
}
